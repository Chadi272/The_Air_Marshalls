    "}\n",
    "\n",
    "fig = gmaps.figure(layout=heat_layout,center=(40.0, -100.0), zoom_level=2)\n",
    "\n",
    "locations = airports[['LATITUDE', 'LONGITUDE']]\n",
    "\n",
    "\n",
    "fig.add_layer(gmaps.heatmap_layer(locations, \n",
    "                                  dissipating=False, \n",
    "                                  max_intensity=100, \n",
    "                                  point_radius=3))\n",
    "fig"
